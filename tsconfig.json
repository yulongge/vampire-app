{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "removeComments": false,
    "preserveConstEnums": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "noImplicitAny": false,
    "allowSyntheticDefaultImports": true,
    "outDir": "lib",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictNullChecks": true,
    "sourceMap": true,
    "baseUrl": ".",
    "rootDir": ".",
    "jsx": "react",
    "allowJs": true,
    "resolveJsonModule": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "paths": {
      "@/api/*": ["./src/apis/*"],
      "@/assets/*": ["./src/assets/*"],
      "@/common/*": ["./src/common/*"],
      "@/components/*": ["/src/components/*"],
      "@/env": ["./src/env"],
      "@/plugins": ["./src/plugins"],
      "@/store/*": ["./src/store/*"],
      "@/utils/*": ["./src/utils/*"]
    },
  },
  "include": ["./src", "global.d.ts"],
  "compileOnSave": false
}
